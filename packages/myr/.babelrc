{
  "assumptions": {
    "setPublicClassFields": true
  },
  "presets": [
    ["@babel/preset-env", {
      "bugfixes": true,
      "modules": "cjs"
    }],
    "@babel/preset-typescript"
  ],
  "plugins": [
    "babel-plugin-transform-typescript-metadata",
    ["@babel/plugin-proposal-decorators", {
      "legacy": true
    }],
    "@babel/proposal-class-properties",
    "macros"
  ]
}